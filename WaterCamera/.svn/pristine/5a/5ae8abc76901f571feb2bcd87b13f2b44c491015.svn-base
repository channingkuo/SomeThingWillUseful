using System;

namespace RekTec.Messages.ViewModels
{
	/// <summary>
	/// 带标记的消息模型
	/// </summary>
	public class BadgeMessageModel
	{
		/// <summary>
		/// 消息的发送者
		/// </summary>
		public string From { get; set; }

		/// <summary>
		/// 消息的数字
		/// </summary>
		public int Badge { get; set; }

		/// <summary>
		/// 消息的时间
		/// </summary>
		public string Time { get; set; }

		/// <summary>
		/// 消息的内容
		/// </summary>
		public string Content { get; set; }

		/// <summary>
		/// 消息的动作
		/// </summary>
		public string Action { get; set; }
	}
}
