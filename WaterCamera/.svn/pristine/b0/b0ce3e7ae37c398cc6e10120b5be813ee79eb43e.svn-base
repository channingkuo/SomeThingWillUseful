<ion-view view-title="设置服务器地址" class="stable-bg">
    <ion-nav-buttons side="right">
        <button class="button button-clear icon ion-ios-checkmark-empty" ng-click="save()"></button>
    </ion-nav-buttons>
    <ion-content>
        <div class="list">
            <label class="item item-input">
                <input ng-model="vm.address" type="text" placeholder="请输入服务器地址"/>
            </label>
        </div>
    </ion-content>
</ion-view>